{
  "markdownlint.config": {
    "MD033": {
      "allowed_elements": [
        "details",
        "summary"
      ]
    }
  },
  "shellcheck.customArgs": [
    "--external-sources"
  ],
  "cSpell.ignoreWords": [
    "Autoenv",
    "CKAD",
    "DRYJ",
    "DRYY",
    "ckadenv",
    "configmap",
    "cpus",
    "devcontainers",
    "dind",
    "envpod",
    "healthz",
    "limitranges",
    "mkcd",
    "persistentvolumeclaims",
    "resourcequotas",
    "securepassword",
    "tasknumber",
    "tolerations",
    "toplevel"
  ]
}
